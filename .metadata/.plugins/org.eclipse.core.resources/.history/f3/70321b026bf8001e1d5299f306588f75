package am;

import java.util.ArrayList;

public class Sub01 {
	public static void run() {
		ArrayList<Car> cars=new ArrayList<Car>();
		Car car1=new Car();
		car1.company="현대";
		car1.model="그랜져";
		car1.color="검정";
		car1.distance=60000;
		car1.date="2021-06";
		cars.add(car1);
		System.out.println("================");
		car1=new Car();
		car1.company="쌍용";
		car1.model="티볼리";
		car1.color="흰색";
		car1.distance=80000;
		car1.date="2019-07";
		cars.add(car1);
		
		for(Car car:cars) {
			car.print();
		}
	}
}
