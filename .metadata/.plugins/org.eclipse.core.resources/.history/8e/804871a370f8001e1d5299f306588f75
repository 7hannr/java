package am;

import java.util.ArrayList;

public class Sub01 {
	public static void run() {
		ArrayList<Car> cars=new ArrayList<Car>();
		
		Car car1=new Car("기아","k7","흰색",100000);
		car1.setColor("흰색");
		car1.setDistance(10000);
		car1.setDate("2021-03");
		cars.add(car1);
		for(Car car:cars) {
			System.out.printf("%s\t%s\t%s\t%d");
			System.out.println("");
		}
	}
}
